{
  "name": "lss64",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "run-s build:*",
    "build:dashboard": "cd src/dashboard && nuxt build --prerender",
    "build:graphics": "cd src/graphics && nuxt build --prerender",
    "build:extension": "tsc -b tsconfig.extension.json",
    "start": "node ../..",
    "postinstall": "nuxt prepare",
    "watch": "run-p watch:*",
